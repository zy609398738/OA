<?xml version="1.0" encoding="UTF-8"?>
<web-fragment
 	xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-fragment_3_0.xsd"
    version="3.0">
        <name>com.bokesoft.r2.extensions.org.tuckey.urlrewriter.UrlRewriteFilter</name>  
		<filter>
	        <filter-name>com.bokesoft.r2.extensions.org.tuckey.urlrewriter.UrlRewriteFilter</filter-name>
	        <filter-class>com.bokesoft.r2.extensions.org.tuckey.urlrewriter.UrlRewriteFilter</filter-class>
         </filter>
		 <filter-mapping>
	        <filter-name>com.bokesoft.r2.extensions.org.tuckey.urlrewriter.UrlRewriteFilter</filter-name>
		    <url-pattern>/*</url-pattern>
		    <dispatcher>REQUEST</dispatcher>
		    <dispatcher>FORWARD</dispatcher>
   	    </filter-mapping>
	    
        <ordering>  
              <before>
                    <others/>  
              </before>  
        </ordering>  
</web-fragment>
