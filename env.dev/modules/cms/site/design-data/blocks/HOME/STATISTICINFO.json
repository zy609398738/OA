{
	"@type":"com.bokesoft.cms2.impl.buildin.modelio.vo.BlockVO",
	"block":{
		"@type":"com.bokesoft.cms2.model.blocks.TmplBlock",
		"formulaText":"cms.PageExp.Local(\"operatorID\", cms.Yigo2MidExp.GetLoginOperator());\ncms.PageExp.Local('EmpID',oa.TodoMidExp.queryEmpIDByID(cms.Yigo2MidExp.GetLoginOperator()));\ncms.PageExp.Local(\n\t\"COUNT_ANNOUNCEMENT\",cms.DataExp.QueryList(\"select count(*) as countQty from OA_NewsDraft_H ndh join OA_NewsType_H nth on ndh.newsTypeID=nth.oid and ndh.status=1200\n\twhere ndh.status=1200 and nth.cmstag=1\")\n);\ncms.PageExp.Local(\n\t\"COUNT_MEETING\",cms.DataExp.QueryList(\"select count(*)  as countQty  from (select a.Topic,a.UrgencyDeg,a.OID,DATE_FORMAT(a.StartTime,'%Y-%m-%d  %H:%i') as starttime,h.name as CREATOR,r.name as addre\n\t\tfrom OA_MeetingInSide_h a LEFT JOIN oa_employee_h h on a.empid=h.oid LEFT JOIN oa_meetingroom_h r on r.oid=a.meetingroom \n\t\twhere a.Status=1200 and Participants like '%\"&cms.PageExp.Local('EmpID')&\"%' order by billdate desc) h\")\n);\ncms.PageExp.Local(\n\t\"COUNT_MAIL\",cms.DataExp.QueryList(\"select count(*)  as countQty  from oa_emailinbox_h h where h.OperatorID=${operatorID} and h.EmailID!='' and isdelete=20 AND EmailStatus=20\")\n);\ncms.PageExp.Local(\n\t\"COUNT_MEDIA\",cms.DataExp.QueryList(\"select count(*) as countQty from OA_NewsDraft_H ndh join OA_NewsType_H nth on ndh.newsTypeID=nth.oid and ndh.status=1200\n\twhere ndh.status=1200 and nth.cmstag=2\")\n);",
		"notes":"",
		"parameters":[],
		"tmplText":"<div class=\"container_showStatisticInfo\">\n\t<div class=\"div_showStatisticInfo\">\n\t\t<div class=\"btn_showStatisticInfo announcementInfo\" data-info=\"announcementInfo\"\n\t\t\tstyle=\"background-image:url('$staticRoot/images/index/announcement.png'); width: 100%; float: right; margin-right: 20px;\" >\n\t\t\t<div class=\"infocontainer\">\n\t\t\t\t<div class=\"infoCount\">\n\t\t\t\t\t<span>$!COUNT_ANNOUNCEMENT[0].COUNTQTY</span>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"infoTitle\">\n\t\t\t\t\t<span>通知公告</span>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n\t<div class=\"div_showStatisticInfo\">\n\t\t<div class=\"btn_showStatisticInfo meetingInfo\" data-info=\"meetingInfo\"\n\t\t\tstyle=\"background-image:url('$staticRoot/images/index/meeting.png');float: right; width: 100%; margin-left: 10px;\" >\n\t\t\t<div class=\"infocontainer\">\t\t\t\n\t\t\t\t<div class=\"infoCount\">\n\t\t\t\t\t<span>$!COUNT_MEETING[0].COUNTQTY</span>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"infoTitle\">\n\t\t\t\t\t<span>会议通知</span>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n\t<div class=\"div_showStatisticInfo\">\n\t\t<div style=\"margin-left: 20px;width: auto\">\n\t\t\t<div class=\"btn_showStatisticInfo mailInfo\" data-info=\"mailInfo\" \n\t\t\t\tstyle=\"background-image:url('$staticRoot/images/index/unreadmail.png');width: 100%;\" >\n\t\t\t\t<div class=\"infocontainer\">\n\t\t\t\t\t<div class=\"infoCount\">\n\t\t\t\t\t\t<span>$!COUNT_MAIL[0].COUNTQTY</span>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"infoTitle\">\n\t\t\t\t\t\t<span>未读邮件</span>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n\t<div class=\"div_showStatisticInfo\">\n\t\t<div style=\"float:right;width:100%\">\n\t\t\t<div style=\"margin-left: 20px;width: auto\">\n\t\t\t\t<div class=\"btn_showStatisticInfo mediaInfo\" data-info=\"mediaInfo\"\n\t\t\t\t\tstyle=\"background-image:url('$staticRoot/images/index/mediareport.png');  width:100%\" >\n\t\t\t\t\t<div class=\"infocontainer\">\n\t\t\t\t\t\t<div class=\"infoCount\">\n\t\t\t\t\t\t\t<span>$!COUNT_MEDIA[0].COUNTQTY</span>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<div class=\"infoTitle\">\n\t\t\t\t\t\t\t<span>媒体报道</span>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n</div>\n<script type=\"text/javascript\">\n\tvar showMoreAnnouncementInfo = function(){\n\t\twindow.location.href = \"newslist.page?typecode=04\";\n\t}\n\tvar showMoreMeetingInfo = function(){\n\t\topenParentEntry('OA/OA/OA_PersonalUse/MeetingManagement/OA_ParticipantMList');\n\t}\n\tvar showMoreMailInfo = function(){\n\t\topenParentEntry('OA/OA/OA_PersonalUse/EmailManagement/OA_EmailInboxView');\n\t}\n\tvar showMoreMediaInfo = function(){\n\t\twindow.location.href = \"newslist.page?typecode=02\";\n\t}\n\t$(\".btn_showStatisticInfo\").click(function(){\n\t\tvar itself = $(this);\n\t\tif(itself.attr(\"data-info\") == \"announcementInfo\"){\n\t\t\tshowMoreAnnouncementInfo();\n\t\t}else if(itself.attr(\"data-info\") == \"meetingInfo\"){\n\t\t\tshowMoreMeetingInfo();\n\t\t}else if(itself.attr(\"data-info\") == \"mailInfo\"){\n\t\t\tshowMoreMailInfo();\n\t\t}else if(itself.attr(\"data-info\") == \"mediaInfo\"){\n\t\t\tshowMoreMediaInfo();\n\t\t}\n\t});\n</script>",
		"type":"TMPL"
	},
	"code":"STATISTICINFO",
	"name":"统计信息",
