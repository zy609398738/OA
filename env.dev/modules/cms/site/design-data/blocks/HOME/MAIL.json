{
	"@type":"com.bokesoft.cms2.impl.buildin.modelio.vo.BlockVO",
	"block":{
		"@type":"com.bokesoft.cms2.model.blocks.TmplBlock",
		"formulaText":"cms.PageExp.Local(\"operatorID\", cms.Yigo2MidExp.GetLoginOperator());\ncms.PageExp.Local(\n\t\"data\",cms.DataExp.QueryList(\"\n           select oid,sender,topic as title,DATE_FORMAT(emailtime,'%Y-%m-%d  %H:%i') as date from oa_emailinbox_h h where h.OperatorID=${operatorID} and h.EmailID!='' and isdelete=20 AND EmailStatus=20 order by emailtime desc limit 7\"\n    )\n)",
		"notes":"",
		"parameters":[],
		"tmplText":"<div>\n   <ul>\n   #foreach($line in $data)\n       <li class=\"mail-list\">\n         <a href=\"\" onclick=\"openParentForm('OA_EmailInbox',$line.OID)\" class=\"f333\">\n             <span class=\"title\" style=\"display:block;\">$line.TITLE</span>\n         </a>\n         <span class=\"sender f999\">$line.SENDER&nbsp;&nbsp;</span>\n         <span style=\"float:right;\" class=\"f999\">$line.DATE</span>\n         <!-- <span class=\"mailicons\" style=\"display:none;\"><b class=\"del\" title=\"删除\" onclick=\"delMail($line.OID)\" style=\"display:none;\"></b></span> -->\n       </li>\n   #end\n   </ul>\n  <a href=\"#\" class=\"a_more\" onclick=\"openParentEntry('OA/OA/OA_PersonalUse/EmailManagement/OA_EmailInboxView')\">更多</a>\n</div>\n<script>\n    $(\".mail-list\").mouseover(function(){\n  \t\t$(this).find(\".mailicons\").show();\n    });\n    $(\".mail-list\").mouseleave(function(){\n  \t\t$(this).find(\".mailicons\").hide();\n    });\n</script>",
		"type":"TMPL"
	},
	"code":"MAIL",
	"name":"首页邮件",
	"params":[]
}