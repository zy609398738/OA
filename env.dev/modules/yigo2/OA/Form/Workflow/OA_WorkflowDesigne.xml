        parent.Container.SetValue(Para("FieldKey1"), GetOID());  
    }
}else{
    parent.Container.SetCellValue(Para("GridKey"), -1, Para("FieldKey1"), GetOID());
    DBUpdate("Update OA_WorkflowType_D set WorkflowDesigneID =? where oid =?",GetOID(),Tag2);
