<Form Key="NJ_UseInventory" Caption="在用盘点情况" FormType="Entity" AbbrCaption="在用盘点情况" InitState="Default">
    <DataSource>
        <DataObject Key="NJ_UseInventory" Caption="在用盘点情况" PrimaryTableKey="NJ_UseInventory" PrimaryType="Entity">
            <TableCollection>
                <Table Key="NJ_UseInventory" Caption="NJ_UseInventory" DBTableName="NJ_UseInventory" TableMode="Detail" SourceType="Query" Persist="false" OrderBy="AssetsCode">
                    <Column Key="OID" Caption="对象标识" DataType="Long"/>
                    <Column Key="SOID" Caption="主对象标识" DataType="Long"/>
                    <Column Key="POID" Caption="父对象标识" DataType="Long"/>
                    <Column Key="VERID" Caption="对象版本" DataType="Integer"/>
                    <Column Key="DVERID" Caption="对象明细版本" DataType="Integer"/>
                    <Column Key="AssetsId" Caption="AssetsId" DataType="Long"/>
                    <Column Key="assetstypeid" Caption="assetstypeid" DataType="Long"/>
                    <Column Key="Model" Caption="Model" DataType="Varchar"/>
                    <Column Key="UNITID" Caption="UNITID" DataType="Long"/>
                    <Column Key="WareHouseId" Caption="WareHouseId" DataType="Long"/>
                    <Column Key="MapCount" Caption="映射数量标记" DataType="Integer"/>
                    <Column Key="UseDeptID" Caption="UseDeptID" DataType="Long"/>
                    <Column Key="UseEmpID" Caption="UseEmpID" DataType="Long"/>
                    <Column Key="MapKey" Caption="映射标识" DataType="Varchar"/>
                    <Column Key="SrcOID" Caption="来源行标识" DataType="Long"/>
                    <Column Key="SrcSOID" Caption="来源表单标识" DataType="Long"/>
                    <Column Key="UseType" Caption="UseType" DataType="Varchar"/>
                    <Column Key="CustomerId" Caption="CustomerId" DataType="Long"/>
                    <Column Key="Room" Caption="Room" DataType="Long"/>
                    <Column Key="AssetsCode" Caption="AssetsCode" DataType="Varchar" SortType="Asc"/>
                    <Column Key="AssetNatureId" Caption="AssetNatureId" DataType="Long"/>
                    <Column Key="UsePeople" Caption="UsePeople" DataType="Varchar"/>
                    <Column Key="AssetsOID" Caption="AssetsOID" DataType="Varchar"/>
                    <Column Key="Amount" Caption="Amount" DataType="Integer"/>
                    <Column Key="CheckDate" Caption="CheckDate" DataType="Long"/>
                    <ParameterCollection>
                        <Parameter DataType="Long" Formula="GetPara('CheckDate_cond')"/>
                        <Parameter DataType="Long" Formula="GetPara('CheckDate_cond')"/>
                    </ParameterCollection>
                    <Statement>
                        <![CDATA[SELECT UseType,UseEmpID,UseDeptID,CustomerId,UsePeople,Room,AssetsId,assetstypeid,WareHouseId,AssetNatureId,Model,UNITID,AssetsCode,CAST(Amount AS SIGNED)Amount,AssetsOID FROM (SELECT * from (select 2 UseType,-1 UseEmpID,-1 UseDeptID,a.CustomerId,a.UsePeople,a.Room,b.AssetsId,b.assetstypeid,b.WareHouseId,b.AssetNatureId,b.Model,b.UNITID,c.AssetsCode,0 Amount,c.OID AssetsOID FROM NJ_SCM_AssetBorrowHead a LEFT JOIN NJ_SCM_AssetBorrowDtl b ON a.oid=b.soid LEFT JOIN NJ_SCM_AssetCode c ON a.OID=c.SOID AND b.AssetsId=c.AssetsBarId UNION ALL select 2 UseType,-1 UseEmpID,-1 UseDeptID,a.CustomerId,a.UsePeople,a.Room,b.AssetsId,b.assetstypeid,b.WareHouseId,b.AssetNatureId,b.Model,b.UNITID,c.AssetsCode,0 Amount,c.OID AssetsOID FROM NJ_SCM_AssetLeasingHead a LEFT JOIN NJ_SCM_AssetLeasingDtl b ON a.oid=b.soid LEFT JOIN NJ_SCM_AssetCode c ON a.OID=c.SOID AND b.AssetsId=c.AssetsBarId UNION ALL select 1 UseType,a.UseEmpID,a.UseDeptID,-1 CustomerId,'' UsePeople,a.Room,b.AssetsId,b.assetstypeid,b.WareHouseId,b.AssetNatureId,b.Model,b.UNITID,c.AssetsCode,0 Amount,c.OID AssetsOID FROM NJ_SCM_AssetCollarHead a LEFT JOIN NJ_SCM_AssetCollarDtl b ON a.oid=b.soid LEFT JOIN NJ_SCM_AssetCode c ON a.OID=c.SOID AND b.AssetsId=c.AssetsBarId UNION ALL select 1 UseType,a.UseEmpID,a.UseDeptID,-1 CustomerId,'' UsePeople,a.Room,b.AssetsBarId,assetstypeid,WareHouseId,AssetNatureId,Model,UNITID,AssetsCode,0 Amount,b.OID AssetsOID from NJ_SCM_AssetTransferHead a LEFT JOIN NJ_SCM_AssetCode b ON a.OID=b.SOID UNION ALL select a.UseType,UseEmpID,UseDeptID,CustomerId,UsePeople,Room,b.AssetsBarId,assetstypeid,WareHouseId,AssetNatureId,Model,UNITID,AssetsCode,0 amount,b.OID AssetsOID from NJ_SCM_UseAssetInventoryHead a LEFT JOIN NJ_SCM_AssetCode b ON a.OID=b.SOID WHERE AssetsOID='') a WHERE AssetsOID NOT IN (SELECT ifnull(SrcOID,AssetsOID) FROM NJ_SCM_AssetCode WHERE ifnull(SrcOID,AssetsOID)>0) AND a.AssetsCode NOT IN (select DISTINCT AssetsCode from NJ_SCM_SweepCodeHead a left join NJ_SCM_SweepCodeDtl b on a.OID=b.SOID WHERE CheckDate=?) UNION ALL select DISTINCT d.UseType,d.UseEmpID,d.UseDeptID,d.CustomerId,d.UsePeople,b.room,c.AssetsBarId,e.assetstypeid,e.WareHouseId,e.AssetNatureId,e.Model,e.UNITID,b.AssetsCode,sum(c.AssetsAmount) Amount,'' AssetsOID from NJ_SCM_SweepCodeHead a left join NJ_SCM_SweepCodeDtl b on a.OID=b.SOID LEFT JOIN NJ_SCM_AssetCode c ON b.AssetsCode=c.AssetsCode LEFT JOIN nj_cp_assets e ON c.AssetsBarId=e.oid LEFT JOIN (SELECT DISTINCT UseType,UseEmpID,UseDeptID,CustomerId,UsePeople,Room from (select 2 UseType,-1 UseEmpID,-1 UseDeptID,a.CustomerId,a.UsePeople,a.Room,a.EmpID,b.AssetsId,b.assetstypeid,b.WareHouseId,b.AssetNatureId,b.Model,b.UNITID,c.AssetsCode,c.OID AssetsOID FROM NJ_SCM_AssetBorrowHead a LEFT JOIN NJ_SCM_AssetBorrowDtl b ON a.oid=b.soid LEFT JOIN NJ_SCM_AssetCode c ON a.OID=c.SOID AND b.AssetsId=c.AssetsBarId UNION ALL select 2 UseType,-1 UseEmpID,-1 UseDeptID,a.CustomerId,a.UsePeople,a.Room,a.EmpID,b.AssetsId,b.assetstypeid,b.WareHouseId,b.AssetNatureId,b.Model,b.UNITID,c.AssetsCode,c.OID AssetsOID FROM NJ_SCM_AssetLeasingHead a LEFT JOIN NJ_SCM_AssetLeasingDtl b ON a.oid=b.soid LEFT JOIN NJ_SCM_AssetCode c ON a.OID=c.SOID AND b.AssetsId=c.AssetsBarId UNION ALL select 1 UseType,a.UseEmpID,a.UseDeptID,-1 CustomerId,'' UsePeople,a.Room,a.EmpID,b.AssetsId,b.assetstypeid,b.WareHouseId,b.AssetNatureId,b.Model,b.UNITID,c.AssetsCode,c.OID AssetsOID FROM NJ_SCM_AssetCollarHead a LEFT JOIN NJ_SCM_AssetCollarDtl b ON a.oid=b.soid LEFT JOIN NJ_SCM_AssetCode c ON a.OID=c.SOID AND b.AssetsId=c.AssetsBarId UNION ALL select 1 UseType,a.UseEmpID,a.UseDeptID,-1 CustomerId,'' UsePeople,a.Room,a.EmpID,b.AssetsBarId,assetstypeid,WareHouseId,AssetNatureId,Model,UNITID,AssetsCode,b.OID AssetsOID from NJ_SCM_AssetTransferHead a LEFT JOIN NJ_SCM_AssetCode b ON a.OID=b.SOID UNION ALL select a.UseType,UseEmpID,UseDeptID,CustomerId,UsePeople,Room,-1 EmpID,b.AssetsBarId,assetstypeid,WareHouseId,AssetNatureId,Model,UNITID,AssetsCode,b.OID AssetsOID from NJ_SCM_UseAssetInventoryHead a LEFT JOIN NJ_SCM_AssetCode b ON a.OID=b.SOID WHERE AssetsOID='') a WHERE AssetsOID NOT IN (SELECT ifnull(SrcOID,AssetsOID) FROM NJ_SCM_AssetCode WHERE ifnull(SrcOID,AssetsOID)>0)) d ON b.Room=d.room WHERE CheckDate=? AND b.AssetsCode NOT IN (SELECT a.AssetsCode from (select 2 UseType,-1 UseEmpID,-1 UseDeptID,a.CustomerId,a.UsePeople,a.Room,b.AssetsId,b.assetstypeid,b.WareHouseId,b.AssetNatureId,b.Model,b.UNITID,c.AssetsCode,c.OID AssetsOID FROM NJ_SCM_AssetBorrowHead a LEFT JOIN NJ_SCM_AssetBorrowDtl b ON a.oid=b.soid LEFT JOIN NJ_SCM_AssetCode c ON a.OID=c.SOID AND b.AssetsId=c.AssetsBarId UNION ALL select 2 UseType,-1 UseEmpID,-1 UseDeptID,a.CustomerId,a.UsePeople,a.Room,b.AssetsId,b.assetstypeid,b.WareHouseId,b.AssetNatureId,b.Model,b.UNITID,c.AssetsCode,c.OID AssetsOID FROM NJ_SCM_AssetLeasingHead a LEFT JOIN NJ_SCM_AssetLeasingDtl b ON a.oid=b.soid LEFT JOIN NJ_SCM_AssetCode c ON a.OID=c.SOID AND b.AssetsId=c.AssetsBarId UNION ALL select 1 UseType,a.UseEmpID,a.UseDeptID,-1 CustomerId,'' UsePeople,a.Room,b.AssetsId,b.assetstypeid,b.WareHouseId,b.AssetNatureId,b.Model,b.UNITID,c.AssetsCode,c.OID AssetsOID FROM NJ_SCM_AssetCollarHead a LEFT JOIN NJ_SCM_AssetCollarDtl b ON a.oid=b.soid LEFT JOIN NJ_SCM_AssetCode c ON a.OID=c.SOID AND b.AssetsId=c.AssetsBarId UNION ALL select 1 UseType,a.UseEmpID,a.UseDeptID,-1 CustomerId,'' UsePeople,a.Room,b.AssetsBarId,assetstypeid,WareHouseId,AssetNatureId,Model,UNITID,AssetsCode,b.OID AssetsOID from NJ_SCM_AssetTransferHead a LEFT JOIN NJ_SCM_AssetCode b ON a.OID=b.SOID UNION ALL select a.UseType,UseEmpID,UseDeptID,CustomerId,UsePeople,Room,b.AssetsBarId,assetstypeid,WareHouseId,AssetNatureId,Model,UNITID,AssetsCode,b.OID AssetsOID from NJ_SCM_UseAssetInventoryHead a LEFT JOIN NJ_SCM_AssetCode b ON a.OID=b.SOID WHERE AssetsOID='') a WHERE AssetsOID NOT IN (SELECT ifnull(SrcOID,AssetsOID) FROM NJ_SCM_AssetCode WHERE ifnull(SrcOID,AssetsOID)>0)) GROUP BY d.UseType,d.UseEmpID,d.UseDeptID,d.CustomerId,d.UsePeople,b.room,c.AssetsBarId,e.assetstypeid,e.WareHouseId,e.AssetNatureId,e.Model,e.UNITID,b.AssetsCode) aa where AssetsId>0]]>
                    </Statement>
