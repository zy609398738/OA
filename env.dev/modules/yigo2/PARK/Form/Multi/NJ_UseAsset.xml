    <DataSource>
        <DataObject Key="NJ_UseAsset" Caption="在用资产" PrimaryTableKey="NJ_UseAsset" PrimaryType="Entity">
            <TableCollection>
                <Table Key="NJ_UseAsset" Caption="NJ_UseAsset" DBTableName="NJ_UseAsset" TableMode="Detail" SourceType="Query" Persist="false" OrderBy="AssetsCode">
                    <Column Key="OID" Caption="对象标识" DataType="Long"/>
                    <Column Key="SOID" Caption="主对象标识" DataType="Long"/>
                    <Column Key="POID" Caption="父对象标识" DataType="Long"/>
                    <Column Key="VERID" Caption="对象版本" DataType="Integer"/>
                    <Column Key="DVERID" Caption="对象明细版本" DataType="Integer"/>
                    <Column Key="AssetsId" Caption="AssetsId" DataType="Long"/>
                    <Column Key="assetstypeid" Caption="assetstypeid" DataType="Long"/>
                    <Column Key="Model" Caption="Model" DataType="Varchar"/>
                    <Column Key="UNITID" Caption="UNITID" DataType="Long"/>
                    <Column Key="Amount" Caption="Amount" DataType="Integer"/>
                    <Column Key="WareHouseId" Caption="WareHouseId" DataType="Long"/>
                    <Column Key="MapCount" Caption="映射数量标记" DataType="Integer"/>
                    <Column Key="UseDeptID" Caption="UseDeptID" DataType="Long"/>
                    <Column Key="UseEmpID" Caption="UseEmpID" DataType="Long"/>
                    <Column Key="MapKey" Caption="映射标识" DataType="Varchar"/>
                    <Column Key="SrcOID" Caption="来源行标识" DataType="Long"/>
                    <Column Key="SrcSOID" Caption="来源表单标识" DataType="Long"/>
                    <Column Key="UseType" Caption="UseType" DataType="Varchar"/>
                    <Column Key="CustomerId" Caption="CustomerId" DataType="Long"/>
                    <Column Key="Room" Caption="Room" DataType="Long"/>
                    <Column Key="AssetsCode" Caption="AssetsCode" DataType="Varchar" SortType="Asc"/>
                    <Column Key="AssetNatureId" Caption="AssetNatureId" DataType="Long"/>
                    <Column Key="UsePeople" Caption="UsePeople" DataType="Varchar"/>
                    <Column Key="EmpID" Caption="EmpID" DataType="Varchar" Length="0"/>
                    <Column Key="AssetsOID" Caption="AssetsOID" DataType="Varchar"/>
                    <Statement>
                        <![CDATA[SELECT * from (select 2 UseType,-1 UseEmpID,-1 UseDeptID,a.CustomerId,a.UsePeople,a.Room,a.EmpID,b.AssetsId,b.assetstypeid,b.WareHouseId,b.AssetNatureId,b.Model,b.UNITID,c.AssetsCode,c.OID AssetsOID FROM NJ_SCM_AssetBorrowHead a LEFT JOIN NJ_SCM_AssetBorrowDtl b ON a.oid=b.soid LEFT JOIN NJ_SCM_AssetCode c ON a.OID=c.SOID AND b.AssetsId=c.AssetsBarId UNION ALL select 2 UseType,-1 UseEmpID,-1 UseDeptID,a.CustomerId,a.UsePeople,a.Room,a.EmpID,b.AssetsId,b.assetstypeid,b.WareHouseId,b.AssetNatureId,b.Model,b.UNITID,c.AssetsCode,c.OID AssetsOID FROM NJ_SCM_AssetLeasingHead a LEFT JOIN NJ_SCM_AssetLeasingDtl b ON a.oid=b.soid LEFT JOIN NJ_SCM_AssetCode c ON a.OID=c.SOID AND b.AssetsId=c.AssetsBarId UNION ALL select 1 UseType,a.UseEmpID,a.UseDeptID,-1 CustomerId,'' UsePeople,a.Room,a.EmpID,b.AssetsId,b.assetstypeid,b.WareHouseId,b.AssetNatureId,b.Model,b.UNITID,c.AssetsCode,c.OID AssetsOID FROM NJ_SCM_AssetCollarHead a LEFT JOIN NJ_SCM_AssetCollarDtl b ON a.oid=b.soid LEFT JOIN NJ_SCM_AssetCode c ON a.OID=c.SOID AND b.AssetsId=c.AssetsBarId UNION ALL select 1 UseType,a.UseEmpID,a.UseDeptID,-1 CustomerId,'' UsePeople,a.Room,a.EmpID,b.AssetsBarId,assetstypeid,WareHouseId,AssetNatureId,Model,UNITID,AssetsCode,b.OID AssetsOID from NJ_SCM_AssetTransferHead a LEFT JOIN NJ_SCM_AssetCode b ON a.OID=b.SOID UNION ALL select a.UseType,UseEmpID,UseDeptID,CustomerId,UsePeople,Room,-1 EmpID,b.AssetsBarId,assetstypeid,WareHouseId,AssetNatureId,Model,UNITID,AssetsCode,b.OID AssetsOID from NJ_SCM_UseAssetInventoryHead a LEFT JOIN NJ_SCM_AssetCode b ON a.OID=b.SOID WHERE AssetsOID='') a WHERE AssetsOID NOT IN (SELECT ifnull(SrcOID,AssetsOID) FROM NJ_SCM_AssetCode WHERE ifnull(SrcOID,AssetsOID)>0)]]>
                    </Statement>
                </Table>
            </TableCollection>
        </DataObject>
    </DataSource>
